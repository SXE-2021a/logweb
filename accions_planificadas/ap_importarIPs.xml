<odoo>
    <data>
        <!--    https://odoo-development.readthedocs.io/en/latest/odoo/models/ir.cron.html -->
        <record  model="ir.cron"  id="importarIPs">
            <field name="name">LogWeb: importar IPs</field>
            <field name="active" eval="True"  />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
        <!--  odoo18     <field name="numbercall">-1</field> --><!--  An integer value specifying how many times the job is executed. A negative value means no limit.-->
            <!--<field name="function">True</field>Odoo Version 12 -->
            <!--<field name="model_id" ref="model_account_invoice" />  Odoo  Version 12 -->
            <field name="model_id" ref="model_logweb_autenticacions" />  <!-- Odoo Version 13 -->
            <field name="state">code</field>
            <field name="code">model.cargaIps()</field>
        <!--   odoo18           <field name="doall" eval="False" /> &lt;!&ndash;      A boolean value indicating whether missed occurrences should be executed when the server restarts&ndash;&gt;-->
        </record>
    </data>
</odoo>